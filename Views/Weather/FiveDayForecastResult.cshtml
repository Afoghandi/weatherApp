@model IEnumerable<dynamic>

	<div>

		<h1>%-Day Weather Forecats</h1>
		<div>
			@foreach(var day in Model){
				<div>
					<h2>@day.Date.ToString("dddd") </h2>
				<i class="wi @day.Icon text-4xl my-4"></i>
				<p>Avg Temp: <strong>@day.AvgTemp°C</strong> </p>
				<p class="text-md">@day.Condition</p>
					</div>
			}
		</div>

	</div>
